pipeline {
    agent {
        node {
            label 'dev'
        }
    }
     tools {
         maven "mymaven"
     }
    stages {
        stage('code') {
            steps {
               git 'https://github.com/Mouryakoti/one.git'
            }
        }
        stage('Build') {
            steps {
               sh 'mvn clean package'
            }
        }
    }
}
